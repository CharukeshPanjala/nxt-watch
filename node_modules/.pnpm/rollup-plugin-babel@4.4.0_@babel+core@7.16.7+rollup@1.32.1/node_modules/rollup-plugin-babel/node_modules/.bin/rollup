#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/dist/bin/rollup/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/dist/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/dist/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rollup@1.32.1/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rollup-plugin-babel@4.4.0_@babel+core@7.16.7+rollup@1.32.1/node_modules/rollup-plugin-babel/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rollup-plugin-babel@4.4.0_@babel+core@7.16.7+rollup@1.32.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@1.32.1/node_modules/rollup/dist/bin/rollup" "$@"
else 
  exec node  "$basedir/../../../../../rollup@1.32.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
