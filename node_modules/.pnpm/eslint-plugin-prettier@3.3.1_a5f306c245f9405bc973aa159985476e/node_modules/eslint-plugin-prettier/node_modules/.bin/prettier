#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/prettier@2.2.1/node_modules/prettier/bin-prettier.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/prettier@2.2.1/node_modules/prettier/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/prettier@2.2.1/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/eslint-plugin-prettier@3.3.1_a5f306c245f9405bc973aa159985476e/node_modules/eslint-plugin-prettier/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/eslint-plugin-prettier@3.3.1_a5f306c245f9405bc973aa159985476e/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.2.1/node_modules/prettier/bin-prettier.js" "$@"
else 
  exec node  "$basedir/../../../../../prettier@2.2.1/node_modules/prettier/bin-prettier.js" "$@"
fi
