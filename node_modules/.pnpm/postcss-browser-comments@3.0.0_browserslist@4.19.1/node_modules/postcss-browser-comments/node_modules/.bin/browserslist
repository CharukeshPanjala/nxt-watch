#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/browserslist@4.19.1/node_modules/browserslist/cli.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/browserslist@4.19.1/node_modules/browserslist/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/browserslist@4.19.1/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/postcss-browser-comments@3.0.0_browserslist@4.19.1/node_modules/postcss-browser-comments/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/postcss-browser-comments@3.0.0_browserslist@4.19.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.19.1/node_modules/browserslist/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../browserslist@4.19.1/node_modules/browserslist/cli.js" "$@"
fi
