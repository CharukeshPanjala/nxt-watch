#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/cli.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/react-router-dom@5.2.0_react@17.0.1/node_modules/react-router-dom/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/react-router-dom@5.2.0_react@17.0.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
fi
