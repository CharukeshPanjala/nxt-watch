#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/nearley@2.20.1/node_modules/nearley/bin/nearleyc.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/nearley@2.20.1/node_modules/nearley/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/nearley@2.20.1/node_modules/nearley/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/nearley@2.20.1/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rst-selector-parser@2.2.3/node_modules/rst-selector-parser/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/rst-selector-parser@2.2.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nearley@2.20.1/node_modules/nearley/bin/nearleyc.js" "$@"
else 
  exec node  "$basedir/../../../../../nearley@2.20.1/node_modules/nearley/bin/nearleyc.js" "$@"
fi
