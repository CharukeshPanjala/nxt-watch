#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/upgrader/bin.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/upgrader/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/husky@4.3.8/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/husky/lib/upgrader/bin.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/husky/lib/upgrader/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/husky/lib/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/husky/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/lib/upgrader/bin.js" "$@"
else 
  exec node  "$basedir/../husky/lib/upgrader/bin.js" "$@"
fi
