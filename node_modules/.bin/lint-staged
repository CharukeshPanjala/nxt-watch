#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/bin/lint-staged.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/lint-staged@10.5.4/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/lint-staged/bin/lint-staged.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/lint-staged/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/lint-staged/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else 
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
