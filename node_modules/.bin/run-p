#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/run-p/index.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/run-p/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/npm-run-all@4.1.5/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/npm-run-all/bin/run-p/index.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/npm-run-all/bin/run-p/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/npm-run-all/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/npm-run-all/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm-run-all/bin/run-p/index.js" "$@"
else 
  exec node  "$basedir/../npm-run-all/bin/run-p/index.js" "$@"
fi
