#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/surge@0.23.0/node_modules/surge/lib/cli.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/surge@0.23.0/node_modules/surge/lib/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/surge@0.23.0/node_modules/surge/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/surge@0.23.0/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/surge/lib/cli.js/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/surge/lib/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/surge/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../surge/lib/cli.js" "$@"
else 
  exec node  "$basedir/../surge/lib/cli.js" "$@"
fi
