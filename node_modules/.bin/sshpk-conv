#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/sshpk-conv/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/sshpk@1.17.0/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/sshpk/bin/sshpk-conv/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/sshpk/bin/node_modules:/home/workspace/reactjs/coding-practices/nxtWatchApp/node_modules/sshpk/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-conv" "$@"
else 
  exec node  "$basedir/../sshpk/bin/sshpk-conv" "$@"
fi
